{
  "name": "traces",
  "private": true,
  "version": "0.0.16",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "doctor": "npx expo-doctor@latest",
    "align-deps": "npx expo install --fix",
    "build-dev-runtime:ios": "eas build --profile development-simulator --platform ios --local",
    "sync-native": "expo prebuild",
    "clean-native": "npm ci && expo prebuild && npx pod-install",
    "test": "tsc --noEmit",
    "lint": "eslint --config .eslintrc.cjs --color '*.ts' 'app/**/*.tsx' 'lib/**/*.ts' 'lib/**/*.tsx' 'web-worker/**/*.ts'",
    "orm": "typeorm-ts-node-esm -d ./dev-data-source.ts",
    "orm:syncdb": "npm run orm -- schema:drop && npm run orm -- schema:sync && npm run orm -- migration:run --fake && npm run orm -- migration:show",
    "postinstall": "npm run bundle-web-worker",
    "bundle-web-worker": "esbuild web-worker/index.ts --bundle --minify --format=iife --banner:js='<script>' --footer:js='</script>' --outfile=assets/web-worker.html",
    "ship:development": "eas update --branch development",
    "ship:prototype": "eas update --branch prototype",
    "ship:production": "eas update --branch production",
    "ship-runtime:development": "eas build -p ios -e development",
    "ship-runtime:prototype": "eas build -p ios -e prototype",
    "ship-runtime:production": "eas build -p ios -e production --auto-submit",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint  --config .eslintrc.cjs --color --cache --fix",
      "tsc-files --noEmit"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/caveat": "^0.2.3",
    "@expo-google-fonts/inter-tight": "^0.2.3",
    "@expo-google-fonts/lexend": "^0.2.3",
    "@expo/html-elements": "^0.6.1",
    "@googlemaps/polyline-codec": "^1.0.28",
    "@react-navigation/drawer": "^6.6.3",
    "@rnmapbox/maps": "^10.0.12",
    "@sentry/react-native": "4.15.2",
    "@turf/turf": "^6.5.0",
    "expo": "~49.0.11",
    "expo-application": "~5.1.1",
    "expo-asset": "~8.9.1",
    "expo-blur": "~12.2.2",
    "expo-build-properties": "~0.6.0",
    "expo-constants": "~14.2.1",
    "expo-dev-client": "~2.2.1",
    "expo-device": "~5.2.1",
    "expo-document-picker": "~11.2.2",
    "expo-file-system": "~15.2.2",
    "expo-font": "~11.1.1",
    "expo-haptics": "~12.2.1",
    "expo-image": "~1.0.1",
    "expo-insights": "~0.2.0",
    "expo-linking": "~4.0.1",
    "expo-location": "~15.1.1",
    "expo-router": "^2.0.8",
    "expo-sensors": "~12.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-sqlite": "~11.1.1",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-task-manager": "~11.1.1",
    "expo-updates": "~0.16.4",
    "metro-config": "^0.79.0",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-console-time-polyfill": "^1.2.3",
    "react-native-geocoder-reborn": "^0.9.0",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-get-random-values": "~1.9.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-ux-cam": "^5.4.10",
    "react-native-webview": "11.26.0",
    "reflect-metadata": "^0.1.13",
    "sentry-expo": "~6.2.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@types/react": "~18.0.27",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "metro-minify-terser": "^0.79.0",
    "pod-install": "^0.1.39",
    "react-native-svg-transformer": "^1.1.0",
    "sharp-cli": "^4.1.1",
    "sqlite3": "^5.1.6",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.4"
  }
}
